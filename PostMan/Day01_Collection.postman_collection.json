							"    pm.expect(jdata.data[2].last_name).to.eql(\"Funke\")\r",
							"    pm.expect(jdata.data[2].avatar).to.eql(\"https://reqres.in/img/faces/9-image.jpg\")\r",
							"})\r",
							"\r",
							"var fname = jdata.data[2].first_name.toLowerCase();\r",
							"var lname = jdata.data[2].last_name.toLowerCase();\r",
							"var fulname=fname.concat(\".\");\r",
							"var fulname1=fulname.concat(lname);\r",
							"var eemail = fulname1.concat(\"@reqres.in\");\r",
							"\r",
							"\r",
							"tests[\"verfiy email\"] = jdata.data[2].email==eemail;\r",
